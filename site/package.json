{
  "name": "react-webpack-boilerplate",
  "description": "Simple boilerplate for a modern JS developer experience.",
  "version": "1.0.0",
  "author": "Jack R. Zhang <15jzhang@gmail.com>",
  "keywords": [],
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "node webpack/devs-server.js",
    "clean": "rm build/bundle.*",
    "build": "NODE_ENV=production webpack --config webpack/config.prod.js",
    "prod": "npm run build && open build/index.html",
    "dev": "npm run clean; open http://localhost:8080 && npm start",
    "lint": "./node_modules/.bin/eslint webpack src",
    "test": "./node_modules/.bin/mocha --reporter spec .specsetup.js src/**/*.spec.js"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "express": "^4.14.0",
    "ignore-styles": "^4.0.0",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.2",
    "postcss-assets": "^4.1.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-url": "^5.1.2",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1"
  },
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  }
}
